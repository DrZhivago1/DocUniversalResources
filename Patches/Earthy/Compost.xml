    <!--Rimworld Farming-->
    <Operation Class="PatchOperation_DZ.FindModByID">
        <mods>
            <li>Storm.RimworldFarming</li>
        </mods>
        <match Class="PatchOperationSequence">
            <success>Always</success>
            <operations>

                <li Class="PatchOperation_DZ.Context">
                    <xpath>/Defs/ThingDef[@Name="DZ_Compost"]</xpath>
                    <operations>
                        <!--Use/replace graphics-->
                        <li Class="PatchOperationReplace">
                            <xpath>/ThingDef/graphicData</xpath>
                            <value>
                                <graphicData>
                                    <texPath>RFCompost</texPath>
                                    <graphicClass>Graphic_Single</graphicClass>
                                </graphicData>
                            </value>
                        </li>
                    </operations>
                </li>

                <!--Remove resource-->
                <li Class="PatchOperationRemove">
                    <xpath>/Defs/ThingDef[defName="RF_Compost"]</xpath>
                </li>

                <!--Replace References-->
                <li Class="PatchOperationReplace">
                    <success>Always</success>
                    <xpath>//thingDef[.="RF_Compost"]</xpath>
                    <value>
                        <thingDef>DZ_Compost</thingDef>
                    </value>
                </li>
                <li Class="PatchOperationReplace">
                    <success>Always</success>
                    <xpath>//li[.="RF_Compost"]</xpath>
                    <value>
                        <li>DZ_Compost</li>
                    </value>
                </li>
                <li Class="PatchOperationSetName">
                    <success>Always</success>
                    <xpath>//RF_Compost</xpath>
                    <name>DZ_Compost</name>
                </li>

            </operations>
        </match>
    </Operation>

